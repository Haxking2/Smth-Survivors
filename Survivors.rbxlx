        self.Abilities[ability].TicksUntilActivation -= 1
        if self.Abilities[ability].TicksUntilActivation <= 0 then
            self.FireAbility:Fire(ability, props, self.Stats)
            self.Abilities[ability].TicksUntilActivation = props.Cooldown
        end
    end
end

function AbilityManager:Update(player)
    local Stats = BASE
    local Abilities = {}
    for item, level in pairs(Players[player].Items) do
